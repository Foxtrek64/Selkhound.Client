<Page
    x:Class="Selkhound.Client.Pages.SettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Selkhound.Client.Pages"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <NavigationView x:Name="SettingsNavView"
                    PaneDisplayMode="Left"
                    IsBackEnabled="True"
                    Header="Settings"
                    BackRequested="SettingsNavView_BackRequested"
                    SelectionChanged="SettingsNavView_SelectionChanged"
                    Loaded="SettingsNavView_Loaded"
                    IsSettingsVisible="False">
        <NavigationView.MenuItems>
            <NavigationViewItem Icon="Accept" Content="Hello world!" />
        </NavigationView.MenuItems>
    </NavigationView>
</Page>
